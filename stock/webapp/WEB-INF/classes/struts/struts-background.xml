<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="background" extends="struts-default" namespace="/background">
		<action name="backIndex" class="IndexAction" method="backGroundIndex">
			<result name="success">/pages/background/index.jsp</result>
		</action>
		<action name="operateUser" class="AppUserAction" method="operateUser">
			<result name="addUserEntry">/pages/background/user/addUser.jsp</result>
			<result name="addUser">/pages/background/user/queryUser.jsp</result>
			<result name="queryUserEntry">/pages/background/user/queryUser.jsp</result>
			<result name="queryUser">/pages/background/user/queryUserResult.jsp</result>
			<result name="updateUserEntry">/pages/background/user/addUser.jsp</result>
			<result name="updateUser">/pages/background/user/queryUser.jsp</result>
			<result name="deleteUser">/pages/background/user/queryUserResult.jsp</result>
		</action>
		<action name="checkUserNameValid" class="AppUserAction" method="checkUserNameValid">
		</action>
		<action name="operateInformation" class="InformationAction" method="operateInformation">
			<result name="addInformationEntry">/pages/background/information/addInformation.jsp</result>
			<result name="addInformation">/pages/background/information/addInformation.jsp</result>
		</action>
		<action name="operateStock" class="StockAction" method="operateStock">
			<result name="addStockEntry">/pages/background/stock/addStock.jsp</result>
			<result name="addStock">/pages/background/stock/queryStock.jsp</result>
			<result name="queryStockEntry">/pages/background/stock/queryStock.jsp</result>
			<result name="queryStock">/pages/background/stock/queryStockResult.jsp</result>
			<result name="deleteStock">/pages/background/stock/queryStockResult.jsp</result>
		</action>
	</package>
</struts>